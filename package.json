{
  "name": "onboard-me-dev",
  "displayName": "Onboard-me-Dev",
  "version": "1.0.0",
  "publisher": "HidaytRahman",
  "description": "Instant dev setup for new projects. Install essential tools, detect project types.",
  "author": "Hidayt Rahman",
  "license": "MIT",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other",
    "Extension Packs"
