  "repository": {
    "type": "git",
    "url": "https://github.com/hidaytrahman/onboard-me-dev.git"
  },
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "onboard.installEssentials",
        "title": "ğŸ§° Install Frontend Essentials"
      },
      {
        "command": "onboard.showWelcome",
        "title": "ğŸ‘‹ Onboard Me dev"
      },
      {
        "command": "onboard.tellJoke",
        "title": "ğŸ˜‚ Tell Me a Dev Joke"
      },
      {
        "command": "onboard.detectProjectType",
        "title": "ğŸ” Detect Project Type"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
